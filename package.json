	"engines": {
		"node": ">= 8.6.0"
	},
	"scripts": {
		"precommit": "npm run build-test:ci",
		"prepublishOnly": "echo DONT PUBLISH IT !!!&&exit 1",
		"ci": "npm run build-test:ci && ncat \"tmp/*/coverage/all/lcov/**/lcov.info\" --banner \"\" | coveralls",
		"lint": "node ./env/scripts/run.js \"require('./env/scripts/common').lint().stopOnError()\"",

		"build:icons": "icon-gen --ico name=appicon --icns name=appicon --favicon name=favicon sizes=16,64,512 --input src/assets/icons/vector/appicon.svg --output src/assets/icons/ico",
		"build:polyfill": "node ./env/scripts/run.js \"require('./env/scripts/common').buildPolyfill().stopOnError()\"",
		"build:libs": "node ./env/scripts/run.js \"require('./env/scripts/common').buildLibs().stopOnError()\"",
		"build:electron:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/builds').buildElectron('dev').stopOnError()\"",
		"build:dev": "node ./env/scripts/run.js \"require('./env/scripts').buildAll(['dev']).stopOnError()\"",

		"test:dev": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['dev'], { build: false }).stopOnError()\"",
		"test:all": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['demo', 'dev', 'stage', 'prod'], { build: false }).stopOnError()\"",
		"test:ci": "node ./env/scripts/run.js \"require('./env/scripts').testCiAll(['dev'], { build: false }).stopOnError()\"",

		"build-test:dev": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['dev']).stopOnError()\"",
		"build-test:all": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['demo', 'dev', 'stage', 'prod']).stopOnError()\"",
		"build-test:ci": "node ./env/scripts/run.js \"require('./env/scripts').testCiAll(['dev']).stopOnError()\"",

		"coverage:check:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageCheck('dev').stopOnError()\"",
		"coverage:mocha:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageMocha('dev').stopOnError()\"",
		"coverage:merge:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageMerge('dev').stopOnError()\"",

		"pack:dev": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['dev'], { test: false, build: false }).stopOnError()\"",
		"pack:demo": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['demo'], { test: false, build: false }).stopOnError()\"",
		"pack:stage": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['stage'], { test: false, build: false }).stopOnError()\"",
		"pack:preprod": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['preprod'], { test: false, build: false }).stopOnError()\"",
		"pack:prod": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['prod'], { test: false, build: false }).stopOnError()\"",
		"pack:chrome:dev": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['chrome'], ['dev'], { test: false, build: false }).stopOnError()\"",
		"pack:chrome:prod": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['chrome'], ['prod'], { test: false, build: false }).stopOnError()\"",
		"pack:all": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['stage', 'dev'], { test: false, build: false }).stopOnError()\"",

		"build-pack:all": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron', 'chrome'], ['stage', 'dev', 'demo'], { test: false }).stopOnError()\"",
		"build-pack:prod": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron', 'chrome'], ['prod'], { test: false }).stopOnError()\"",
		"build-pack:preprod": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['preprod'], { test: false }).stopOnError()\"",
		"build-pack:stage": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron', 'chrome'], ['stage'], { test: false }).stopOnError()\"",
		"build-pack:dev": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['dev'], { test: false }).stopOnError()\"",
		"build-pack:demo": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['demo'], { test: false }).stopOnError()\"",
		"build-pack:chrome:dev": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['chrome'], ['dev'], { test: false }).stopOnError()\"",

		"test-pack:dev": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['dev'], { build: false }).stopOnError()\"",
		"test-pack:all": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['demo', 'dev', 'stage', 'prod'], { build: false }).stopOnError()\"",

		"build-test-pack:dev": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['dev']).stopOnError()\"",
		"build-test-pack:all": "node ./env/scripts/run.js \"require('./env/scripts').packAll(['electron'], ['demo', 'dev', 'stage', 'prod']).stopOnError()\"",

		"deploy:dev": "node ./env/scripts/run.js \"require('./env/scripts').deployAll(['dev'], { test: false, build: false }).stopOnError()\"",
		"build-deploy:dev": "node ./env/scripts/run.js \"require('./env/scripts').deployAll(['dev'], { test: false }).stopOnError()\"",
		"build-test-deploy:dev": "node ./env/scripts/run.js \"require('./env/scripts').deployAll(['dev']).stopOnError()\"",

		"sapper:dev": "node ./env/sapper/dev.js",
		"sapper:dev:components": "node ./env/sapper/dev-components.js",

		"electron:dev": "set APP_CONFIG=debug&& electron ./src/main/node/electron/run/dev/run.js",
		"electron:dev-vm": "set APP_CONFIG=debug&& electron ./src/main/node/electron/run/dev-vm/run.js",
		"electron:export": "set APP_CONFIG=dev&& electron ./src/main/node/electron/run/export/run.js"
	},
