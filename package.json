{
	"name": "app-template",
	"version": "0.0.1",
	"description": "App Template",
	"author": {
		"name": "App Template",
		"url": "https://github.com/AppTemplate/"
	},
	"homepage": "https://github.com/AppTemplate/app-template/",
	"bugs": {
		"url": "https://github.com/AppTemplate/app-template/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:AppTemplate/app-template.git"
	},
	"license": "CC0-1.0",
	"keywords": [],
	"files": [
		"dist/**",
		"src/main/gyp/**"
	],
	"main": "dist/dev/js/main/node/main.js",
	"jsnext:main": "dist/dev/mjs/main/node/main.js",
	"module": "dist/dev/mjs/main/node/main.js",
	"engines": {
		"node": ">= 8.6.0"
	},
	"scripts": {
		"precommit": "npm run build-test:ci",
		"prepare": "npm run build:dev",
		"prepublishOnly": "npm run test:dev",
		"ci": "npm run build-test:ci && ncat \"tmp/common/coverage/all/lcov/**/lcov.info\" --banner \"\" | coveralls",
		"lint": "node ./env/scripts/run.js \"require('./env/scripts/common').lint().stopOnError()\"",
		"build:polyfill": "node ./env/scripts/run.js \"require('./env/scripts/common').buildPolyfill().stopOnError()\"",
		"build:libs": "node ./env/scripts/run.js \"require('./env/scripts/common').buildLibs().stopOnError()\"",
		"build:dev": "node ./env/scripts/run.js \"require('./env/scripts').buildAll('dev').stopOnError()\"",
		"build:gyp": "node ./env/scripts/run.js \"require('./env/scripts/common').buildGyp().stopOnError()\"",
		"test:dev": "node ./env/scripts/run.js \"require('./env/scripts').testAll('dev').stopOnError()\"",
		"test:all": "node ./env/scripts/run.js \"require('./env/scripts').testAll('demo', 'dev', 'stage', 'prod').stopOnError()\"",
		"test:ci": "node ./env/scripts/run.js \"require('./env/scripts').testCiAll('dev').stopOnError()\"",
		"build-test:dev": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['dev']).stopOnError()\"",
		"build-test:all": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['demo', 'dev', 'stage', 'prod']).stopOnError()\"",
		"build-test:ci": "node ./env/scripts/run.js \"require('./env/scripts').testCiAll(['dev']).stopOnError()\"",
		"coverage:mocha:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageMocha('dev').stopOnError()\""
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-export-default-from": "^7.5.2",
		"@babel/plugin-proposal-export-namespace-from": "^7.5.2",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/plugin-proposal-throw-expressions": "^7.2.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-async-to-generator": "^7.5.0",
		"@babel/plugin-transform-parameters": "^7.4.4",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/plugin-transform-typescript": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/register": "^7.5.5",
		"@babel/runtime-corejs3": "^7.5.5",
		"@typescript-eslint/eslint-plugin": "^3.0.1",
		"@typescript-eslint/eslint-plugin-tslint": "^3.0.1",
		"@typescript-eslint/parser": "^3.0.1",
		"babel-eslint": "^10.0.3",
		"babel-plugin-root-import": "^6.5.0",
		"caller": "^1.0.1",
		"caniuse-lite": "^1.0.30001061",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"chokidar": "^3.1.0",
		"color": "^3.1.2",
		"core-js": "^3.2.1",
		"core-js-pure": "^3.2.1",
		"cpy-cli": "^2.0.0",
		"depcheck": "^0.8.3",
		"electron": "^6.0.11",
		"eslint": "^7.1.0",
		"eslint-config-pro": "^1.0.1",
		"eslint-plugin-markdown": "^1.0.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-sonarjs": "^0.5.0",
		"fs-extra": "^8.1.0",
		"globby": "^10.0.1",
		"istanbul-combine": "^0.3.0",
		"json5": "^2.1.0",
		"kleur": "^3.0.3",
		"mocha": "^5.2.0",
		"ncat": "^2.0.7",
		"node-gyp": "^5.1.0",
		"nyc": "^14.1.1",
		"ps-tree": "^1.2.0",
		"rdtsc": "^2.0.0",
		"regenerator-runtime": "^0.13.3",
		"require-from-memory": "^1.0.2",
		"rollup": "^1.27.11",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-istanbul": "^2.0.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-terser": "^5.1.3",
		"rollup-pluginutils": "^2.8.2",
		"shx": "^0.3.2",
		"tslint": "^5.20.0",
		"tslint-clean-code": "^0.2.10",
		"tslint-consistent-codestyle": "^1.15.1",
		"tslint-eslint-rules": "^5.4.0",
		"tslint-no-circular-imports": "^0.7.0",
		"tslint-sonarts": "^1.9.0",
		"typescript": "^3.6.3",
		"webrain": "^1.0.1-a"
	}
}
